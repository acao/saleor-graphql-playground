{
  "name": "saleor-graphql-playground",
  "version": "1.0.0",
  "private": true,
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "rimraf dist && concurrently --kill-others \"npm:dev:*\"",
    "dev:server": "web-dev-server --app-index index.html --node-resolve",
